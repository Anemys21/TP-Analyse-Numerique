function[y]=f(x)
y=x^6-x-1
 endfunction
function[y]=fprime(x)
    
    y=6*x^5-1
endfunction
function[x]=newton(b,e)
    
    x=b;
    while abs(f(x))>e
        x=x-(f(x)/fprime(x));
    end
    
       
 endfunction
 b=1; e=10^-2;
 x=newton(b,e)
 disp('la solution de l''equation est :x='+string(x))
